<body>

    <% include ./partials/header.html %>

    <!--<div class="cell listing-images" style="background-image: url(<%= listing.thumbnail_img %>)"></div>-->
    <div class="cell listing-images" style="background-image: url('/images/owl.jpg')"></div>

    <div class="waste">
        <div class="responsive-container">
            <div class="grid padding-free relative-positioned">
                <div class="cell listing-scroll-summary">
                    <div class="listing-scroll-summary--inner">
                        Price & Summary
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="responsive-container">
        <div class="grid padding-free">
            <div class="cell column-4">
                <div class="newcol">
                    <div class="gap"></div>
                    <div class="form grid">
                        <form id="my-form" method="post" action="/booking">
                            <div class="content">
                                <div class="form-row">
                                    <div class="grid">
                                        <div class="cell half-field">
                                            <div class="form-description"><span>Check-in</span></div>
                                            <input readonly name="checkin" id="checkin" class="input-field" type="text" placeholder="dd/mm/yyyy" style="border-bottom-right-radius: 0; border-top-right-radius: 0">
                                        </div>
                                        <div class="cell half-field">
                                            <div class="form-description"><span>Check-out</span></div>
                                            <input readonly name="checkin" id="checkout" class="input-field" type="text" placeholder="dd/mm/yyyy" style="border-bottom-left-radius: 0; border-top-left-radius: 0">
                                        </div>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="grid">
                                        <div class="form-description"><span>Number of Guests</span></div>
                                        <input name="max_guest" class="input-field" type="number" placeholder="Number of Guests">
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="grid">
                                        <div class="form-description"><span><b>Price: </b><%= listing.price %></span></div>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="grid padding-free">
                                        <button type="submit" class="submit-btn">Book</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="cell column-6">
