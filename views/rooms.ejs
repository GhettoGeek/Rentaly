                            <div class="scl">
                                <a id="first-link" href="#details">Details</a>
                                <a id="second-link" href="#host">Host</a>
                                <a id="third-link" href="#reviews">Reviews</a>
                                <a id="fourth-link" href="#location">Location</a>
                            </div>
                        </nav>
                    </div>
